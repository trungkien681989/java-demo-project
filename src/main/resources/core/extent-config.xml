<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>Demo Report</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Demo Report</reportName>

        <!-- report headline - displayed at top-nav, after reportHeadline -->
        <reportHeadline></reportHeadline>

        <!-- global date format override -->
        <!-- defaults to yyyy-MM-dd -->
        <dateFormat>yyyy-MM-dd</dateFormat>

        <!-- global time format override -->
        <!-- defaults to HH:mm:ss -->
        <timeFormat>HH:mm:ss</timeFormat>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
        $(document).ready(function() {
          
        });
      ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[
                test-name, category-name  {display: inline-block; word-break: break-word;}
                td {word-break: break-word;}
                blockquote {
                    border-left: 10px solid #ccc; margin: 1.5em 10px; padding: 0.5em 10px;
                    quotes: "\201C""\201D""\2018""\2019";
                }
            ]]>
        </styles>
    </configuration>
</extentreports>
